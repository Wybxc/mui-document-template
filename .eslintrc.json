{
  "extends": [
    "next/core-web-vitals",
    "google"
  ],
  "rules": {
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "valid-jsdoc": [
      "error",
      {
        "requireParamType": false,
        "requireReturnType": false
      }
    ],
    "sort-imports": [
      "error"
    ]
  }
}